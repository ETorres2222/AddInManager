<Page x:Class="RevitAddinManager.View.Control.FrmDockablePanel"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:control="clr-namespace:RevitAddinManager.View.Control"
      mc:Ignorable="d"
      Background="White"
      MinWidth="350"
      MinHeight="450"
      Title="FrmDockablePanel" Height="800" Width="250">
    <Page.Resources>
        <ResourceDictionary>
            <!--  Load this specific theme because the Aero theme for CheckBox has issues.  -->
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/RevitAddinManager;component\Themes\Styles/LightTheme.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Page.Resources>
    <Grid Background="{DynamicResource UserControlBackground}">
        <control:LogControl></control:LogControl>
    </Grid>
</Page>